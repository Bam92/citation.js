{
  "plugins": ["plugins/markdown"],
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "includePattern": "(?:.+-0\\.2\\.js(doc|x)?|index.js)$",
    "excludePattern": "(^|\\/|\\\\)_|b\\.js(doc|x)?|docs\\/src\\/.+?\\.js$"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
} 
