{
  "name": "citation-js",
  "version": "0.2.15",
  "description": "Citation.js converts formats like BibTeX, Wikidata JSON and ContentMine JSON to CSL-JSON to convert to other formats like APA, Vancouver and back to BibTeX.",
  "main": "index.js",
  "directories": {
    "doc": "docs/api/",
    "test": "docs/test/spec/",
    "lib": "src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larsgw/citation.js.git"
  },
  "keywords": [
    "citation",
    "bibtex",
    "wikidata",
    "contentmine",
    "quickscrape",
    "csl",
    "citeproc"
  ],
  "dependencies": {
    "commander": "~2.9.0",
    "striptags": "~2.1.1",
    "sync-request": "~3.0.1",
    "wikidata-sdk": ">=4.2.0"
  },
  "devDependencies": {
    "jasmine-node": "~1.14.5",
    "jsdoc": "~3.4.2"
  },
  "scripts": {
    "test": "node_modules/.bin/jasmine-node ./docs/test/spec/citation-0.2.spec.js"
  },
  "author": "Lars Willighagen (https://larsgw.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larsgw/citation.js/issues"
  },
  "homepage": "https://github.com/larsgw/citation.js#readme"
}
